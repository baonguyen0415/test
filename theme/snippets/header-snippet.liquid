                </div>
              {%- endif -%}
              {%- render 'header-snippet' type: 'cart' -%}
            </div>
          </div>
        </div>
      </div>

      <div class="header-bottom">
        <div class="{{ layout_class }}">
          <div class="header-bottom_wrapper d-flex">

            <div class="header-bottom-lr">
              <div class="header-bottom_left">
                {%- if section.settings.menu != blank -%}
                {%- render 'header-snippet' type: 'navigation', section: section -%}
                {%- endif -%}
              </div>
              {%- unless section.settings.textbox == blank -%}
                <div class="header-bottom_right">
                  <span>{{ section.settings.textbox }}</span>
                </div>
              {%- endunless -%}
            </div>

          </div>
        </div>
      </div>
    </div>
  {% comment %} end {% endcomment %}

  {%- when 'desk-10' -%}
    {%- liquid
      assign layout = settings.layout
      if layout == 'boxed'
        assign layout_class = 'container'
      else
        assign layout_class = 'container-fluid'
      endif
    -%}

    <div class="header-desk d-none d-lg-block">
      <div class="header-top">
        <div class="{{ layout_class }}">

          <div class="header-top_wrapper d-flex justify-content-between">
            <div class="header-logo-vertical-menu">
              {%- render 'header-snippet' type: 'logo', section: section -%}
              {%- if section.settings.toggle_menu != blank -%}
                {%- render 'header-snippet' type: 'third-nav' -%}
              {%- endif -%}
            </div>

            {%- if section.settings.menu != blank -%}
              {%- render 'header-snippet' type: 'navigation', section: section -%}
            {%- endif -%}

            <div class="header-action-list d-flex">
              {%- render 'header-snippet' type: 'search-icon' section: section -%}
              {%- if settings.enable_product_compare -%}
                <div class="compare-target">
                  {%- render 'arn_wl_cp_buttons', type: 'cp_show_btn', product_handle: product.handle -%}
                </div>
              {% endif %}

              {%- if settings.enable_product_wishlist -%}
                <div class="wishlist-target">
                  {%- render 'arn_wl_cp_buttons', type: 'wl_show_btn', product_handle: product.handle -%}
                </div>
              {%- endif -%}
              {%- render 'header-snippet' type: 'cart' -%}
            </div>
          </div> 

          <script type="text/javascript">
            let searchIconOpen = document.querySelector('.search-icon-open');
            let searchIconClose = document.querySelector('.search-icon-close');

            searchIconOpen.addEventListener("click", (e) => {
              document.querySelector('.header-search-icon').classList.add('active');
              document.querySelector('.d-header-search').classList.add('active');
            });

            searchIconClose.addEventListener("click", (e) => {
              document.querySelector('.header-search-icon').classList.remove('active');
              document.querySelector('.d-header-search').classList.remove('active');
            });
          </script>

        </div>
      </div>
    </div>
  {% comment %} end {% endcomment %}

  {%- when 'mobile' -%}
    {%- assign menu = section.settings.menu_mobile -%}
    <div class="header-mobile d-lg-none">
      <div class="container">
        <div class="header-mobile_top">
          <label class="header-hamburger" for="mobile-drawer-input">
            {%- render 'svg' type: 'hamburger' -%}
          </label>
          {%- render 'header-snippet' type: 'logo-mobile', section: section -%}
          <div class="header-icons">
            <ul class="icon-list">
              <li class="icon-item m-icon-search">
                <span class="mis-open">{%- render 'svg' type: 'search' -%}</span>
                <span class="mis-close">{%- render 'svg' type: 'close' -%}</span>

                <script type="text/javascript">
                  let misOpen = document.querySelector('.mis-open');
                  let mClose = document.querySelector('.mis-close');

                  misOpen.addEventListener("click", (e) => {
                    document.querySelector('.m-icon-search').classList.add('active');
                    document.querySelector('.m-header-search').classList.add('active');
                  });

                  mClose.addEventListener("click", (e) => {
                    document.querySelector('.m-icon-search').classList.remove('active');
                    document.querySelector('.m-header-search').classList.remove('active');
                  });
                </script>
              </li>
              {%- if shop.customer_accounts_enabled or shop.customer_accounts_optional -%}
                <li class="icon-item icon-user">
                  <a href="{%- if customer -%} {{routes.account_url}} {%else%} {{routes.account_login_url}} {%- endif -%}">
                    {%- render 'svg' type: 'user' -%}
                  </a>
                </li>
              {%- endif -%}

              <li class="icon-item icon-header-cart">
                <a href="{{routes.cart_url}}" class="position-relative js-header-cart-icon--mobile">
                  {%- render 'svg' type: 'cart' -%}
                  <span class="number position-absolute js-cart-item-count">{{cart.item_count}}</span>
                </a>
              </li>
            </ul>
          </div>
        </div>

        <div class="header-mobile_bottom">
          <div class="header-search m-header-search">
            <search-form>
              <form action="/search" class="search-form" methos="get">
                <input type="hidden" value="{{settings.search_type}}" name="type">
                <input type="hidden" value="{{settings.search_unavailable_products}}" name="options[unavailable_products]">
                <input type="hidden" value="last" name="options[prefix]">
                <input type="text" name="q" placeholder="{{ 'sections.header.search_placeholder' | t}}" autocomplete="off">
                <button type="submit" class="btn btn-2">
                  {%- render 'svg' type: 'search' -%}
                  {%- render 'svg' type: 'is-loading' -%}
                </button>
              </form>
              <div class="search-results">
                <div class="search-results_list">
                  <div class="search-results_list-inner js-results-container"></div>
                  <button class="btn btn-1 w-100 js-view-all">{{ 'general.search.view_all' | t}}</button>
                </div>
                <div class="search-results_empty text-center">{{ 'general.search.empty' | t}}</div>
                <div class="search-results_loading w-100 text-center">{%- render 'svg' type: 'is-loading' -%}</div>
              </div>
            </search-form>
          </div>
        </div>
      </div>

      <input type="checkbox" class="d-none" id="mobile-drawer-input">
      <template id="header-mobile-drawer-wrapper">
        <div class="drawer drawer--left header-mobile_drawer" id="header-mobile-drawer">
          <div class="mobile-layout-bar">
            <div class="m-block-icons">
              <label class="drawer-close" for="mobile-drawer-input">
                {%- render 'svg' type: 'close' -%}
              </label>
              <ul class="icon-list">
                {%- if settings.enable_product_compare -%}
                  <li class="compare-target">
                    {%- render 'arn_wl_cp_buttons', type: 'cp_show_btn', product_handle: product.handle -%}
                  </li>
                {% endif %}

                {%- if settings.enable_product_wishlist -%}
                  <li class="wishlist-target">
                    {%- render 'arn_wl_cp_buttons', type: 'wl_show_btn', product_handle: product.handle -%}
                  </li>
                {%- endif -%}
              </yl>
            </div>
          </div>

          {%- if settings.enable_language_selector -%}
            {%- if settings.enable_currency_selector -%}
              {%- assign check_lc = 'lc-two' -%}
            {%- else -%}
              {%- assign check_lc = 'lc-one' -%}
            {%- endif -%}
          {%- else -%}
            {%- if settings.enable_currency_selector -%}
              {%- assign check_lc = 'lc-one' -%}
            {%- else -%}
              {%- assign check_lc = '' -%}
            {%- endif -%}
          {%- endif -%}
          <div class="drawer-wrapper {{ check_lc }}">
            <ul class="menu-list menu-mobile-list style-{{ settings.mobile_touch_menu}}">
              {%- for link in linklists[menu].links -%}
                {%- capture mega_capture -%}
                  {%- render 'header-mega-mobi' section: section, link_title: link.title -%}
                {%- endcapture -%}

                {%- capture menu_panel_id -%}menu-panel-{{link.id}}-{{forloop.index}}{%- endcapture -%}

                {%- if mega_capture != blank -%}
                  <li class="menu-item menu-mobile-item">
                    <div class="menu-item_wrap">
                      <a href="{{link.url}}">{{link.title}}</a>
                      <label for="{{menu_panel_id}}">
                        {%- render 'svg' type: 'angle-right' -%}
                      </label>
                    </div>
                    <input type="checkbox"  id="menu-panel-{{link.id}}-{{forloop.index}}" class="d-none menu-checkbox-mobile">
                    {{mega_capture | replace_first: '[menu-panel-id]', menu_panel_id}}
                  </li>

                {%- else -%}
                  <li class="menu-item menu-mobile-item">
                    {%- if link.links != blank -%}
                      <div class="menu-item_wrap">
                        <a href="{{link.url}}">{{link.title}}</a>
                        <label for="{{menu_panel_id}}">
                          {%- render 'svg' type: 'angle-right' -%}
                        </label>
                      </div>
