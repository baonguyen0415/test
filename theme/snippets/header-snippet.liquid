  {% comment %} end {% endcomment %}

  {%- when 'desk-8' -%}
    {%- liquid
      assign layout = settings.layout
      if layout == 'boxed'
        assign layout_class = 'container'
      else
        assign layout_class = 'container-fluid'
      endif
    -%}

    <div class="header-desk d-none d-lg-block">
      <div class="header-top">
        <div class="{{ layout_class }}">
          <div class="header-top_wrapper d-flex justify-content-between">
            <div class="header-logo-vertical-menu">
              {%- render 'header-snippet' type: 'logo', section: section -%}
              {%- if section.settings.toggle_menu != blank -%}
                {%- render 'header-snippet' type: 'third-nav' -%}
              {%- endif -%}
            </div>

            <div class="header-search-menu">
              {%- render 'header-snippet' type: 'search-form' section: section -%}

              {%- if section.settings.textbox_1 != blank or section.settings.textbox_2 != blank -%}
                <div class="header-shipping-text">
                  {%- if section.settings.textbox_1 != blank -%}
                    <span class="first">{{ section.settings.textbox_1 }}</span>
                  {%- endif -%}

                  {%- if section.settings.textbox_2 != blank -%}
                    <span class="second">{{ section.settings.textbox_2 }}</span>
                  {%- endif -%}
                </div>
              {%- endif -%}
            </div>

