    {%- assign menu_label_handles_3 = _work_handles_label_3 | split: '[|||]' -%}

    <div class="header-bottom_vertical-menu vertical-nav desk-lazy-menu" id="header-second-nav">
      <div class="wrapper vertical-menu-head head">
        {%- render 'svg', type: 'list-1' -%}
        <span>{{ 'sections.header.all_departments' | t }}</span>
        {%- render 'svg' type: 'angle-down' -%}
      </div>

      <div class="header-vertical-menu dropdown-menu">
        <ul class="menu-list">

          {%- for link in linklists[vertical_menu].links -%}
            {%- capture mega_capture -%}
            {%- render 'header-mega' section: section, link_title: link.title -%}
            {%- endcapture -%}

            {%- if mega_capture != blank -%}
              <li class="menu-item mega-menu-item{% if _highlight_menu contains link.title %} highlight{% endif %}">
                <div class="menu-item_wrapper">
                  <a href="{{link.url}}">
                    <span>{{link.title}}</span>
                    {% capture _menu_label %}
                      {% if menu_label_handles_1 contains link.title %}{{menu_label_html_1}}{% endif %}
                      {% if menu_label_handles_2 contains link.title %}{{menu_label_html_2}}{% endif %}
                      {% if menu_label_handles_3 contains link.title %}{{menu_label_html_3}}{% endif %}
                    {% endcapture %}
                    {% if _menu_label != blank %}<span class="labels-wrapper">{{_menu_label}}</span>{% endif %}
                  </a>
                  <span class="svg_element-ui">
                    {%- render 'svg' type: 'angle-right' -%}
                  </span>
                </div>
                <template class="temp-id">{{mega_capture}}</template>
              </li>
            {%- else -%}
              <li class="menu-item{% if link.links != blank %} mega-menu-item{% endif %}{% if _highlight_menu contains link.title %} highlight{% endif %}">
                {%- if link.links != blank -%}
                  <div class="menu-item_wrapper">
                    <a href="{{link.url}}">
                      <span>{{link.title}}</span>
                      {% capture _menu_label %}
                        {% if menu_label_handles_1 contains link.title %}{{menu_label_html_1}}{% endif %}
                        {% if menu_label_handles_2 contains link.title %}{{menu_label_html_2}}{% endif %}
                        {% if menu_label_handles_3 contains link.title %}{{menu_label_html_3}}{% endif %}
                      {% endcapture %}
                      {% if _menu_label != blank %}<span class="labels-wrapper">{{_menu_label}}</span>{% endif %}
                    </a>
                    <span class="svg_element-ui">
                      {%- render 'svg' type: 'angle-right' -%}
                    </span>
                  </div>
                  <template class="temp-id">{%- render 'header-sublinks' link: link -%}</template> 
                {%- else -%}
                  <a href="{{link.url}}">
                    <span>{{link.title}}</span>
                    {% capture _menu_label %}
                      {% if menu_label_handles_1 contains link.title %}{{menu_label_html_1}}{% endif %}
                      {% if menu_label_handles_2 contains link.title %}{{menu_label_html_2}}{% endif %}
                      {% if menu_label_handles_3 contains link.title %}{{menu_label_html_3}}{% endif %}
                    {% endcapture %}
                    {% if _menu_label != blank %}<span class="labels-wrapper">{{_menu_label}}</span>{% endif %}
                  </a>
                {%- endif -%}
              </li>
            {%- endif -%}
          {%- endfor -%}
          
      </div>
    </div>

  {%- when 'third-nav' -%}
    {%- assign toggle_menu = section.settings.toggle_menu -%}

    <div class="off-canvas-navigation-wrapper">
      <div class="wrapper toggle-menu-head">
        <div class="off-canvas-navbar-toggle-buttons">
          <div class="navbar-toggle-hamburger">
            {%- render 'svg', type: 'hamburger' -%}
          </div> 
          <div class="navbar-toggle-close">
            {%- render 'svg', type: 'close' -%}
          </div>
        </div>

        <div class="header-toggle-menu">
          <ul class="menu-list">
            {%- for link in linklists[toggle_menu].links -%}
              <li class="menu-item third-menu-item">
                {%- if link.links != blank -%}
                  <div class="menu-item_wrapper third-miw">
                    <a href="{{link.url}}">{{link.title}}</a>
                    {%- render 'svg' type: 'angle-down' -%}
                  </div>
                  {%- render 'header-sublinks' link: link -%}
                {%- else -%}
                  <a href="{{link.url}}">{{link.title}}</a>
                {%- endif -%}
              </li>

            {%- endfor -%}
          </ul>
        </div>
      </div>
    </div>

    <script type="text/javascript">
    (() => {
      [...document.getElementsByClassName('third-miw')].forEach(button => {
        button.addEvent('click', function() {
          if(this.closest('.menu-item').classList.contains('active')){
            let allbutton = document.getElementsByClassName('third-menu-item');
              for (let i = 0; i < allbutton.length; i++) {
                allbutton[i].classList.remove('active');
            }
          }else{
            let allbutton = document.getElementsByClassName('third-menu-item');
              for (let i = 0; i < allbutton.length; i++) {
                allbutton[i].classList.remove('active');
            }
            this.closest('.menu-item').classList.add('active');
          }
        })
      })
    })()

    </script>

  {%- when 'dropdown-cart' -%}
    {%- if template.name != 'cart' or settings.cart_type != 'page' -%}
      <template id="dropdown-cart-template">
        <div class="dropdown-cart js-header-dropdown-cart" data-cart-item-count="{{cart.item_count}}">
          <form action="/cart" method="post" class="cart-form">
            <header class="dropdown-cart_title dropdown-cart_border-bottom d-flex justify-content-between align-items-center">
              <span>{{ 'sections.dropdown_cart.title' | t}}</span>
              <button type="button" class="btn-close js-btn-close cursor-pointer" title="{{ 'sections.dropdown_cart.close' | t }}">{%- render 'svg' type: 'close' -%}</button>
            </header>

            <main class="dropdown-cart_body cart-item-list js-cart-line-item-list dropdown-cart_border-bottom position-relative">
              {%- for line_item in cart.items -%}
                <div class="cart-item-list_line-item cart-line-item js-cart-line-item" data-key="{{line_item.key}}">
                  <div class="cart-line-item_info d-flex justify-content-between align-items-center">
                    <a href="{{line_item_.url}}" class="cart-line-item_img">
                      {%- render 'image-style-lazy' image: line_item.image, limit: 100 -%}
                    </a>
                  </div>
                  <div class="cart-line-item_info_content">
                    <a href="{{line_item.url}}" class="cart-line-item_title">{{line_item.product.title}}</a>
                    <div class="cart-line-item_wrapper">
                      <span class="cart-line-item_variant{% if line_item.variant.title == 'Default Title' %} d-none{% endif %}">{{ line_item.variant.title }}</span>
                      <span class="cart-line-item_qty">{{ 'sections.dropdown_cart.qty' | t}} <span class="js-line-item-qty">{{line_item.quantity}}</span></span>
                      <div class="art-line-item_price_remove">
                        <span class="cart-line-item_price js-line-item-price">{{line_item.price | money}}</span>
                        <a href="{{routes.cart_change_url}}?quantity=0&id={{line_item.key}}" class="btn btn-remove js-btn-remove">{{ 'sections.dropdown_cart.remove' | t}}</a>
                      </div>
                    </div>
                  </div>
                </div>
              {%- endfor -%}
            </main>
            <footer class="dropdown-cart_footer">
              <div class="cart-total dropdown-cart_border-bottom d-flex align-items-center justify-content-between">
                <span class="cart-total_label">{{ 'sections.dropdown_cart.cart_total' | t}}</span>
                <div class="cart-total_price js-cart-total">{{cart.total_price | money}}</div>
              </div>
              <div class="cart-buttons d-flex align-items-center justify-content-between">
                <a href="{{routes.cart_url}}" class="btn btn-2 btn-review-cart">{{ 'sections.dropdown_cart.review_cart' | t}}</a>
                <button type="submit" name="checkout" class="btn btn-1 btn-submit">{{ 'sections.dropdown_cart.checkout' | t}}</button>
              </div>
            </footer>
          </form>
          <div class="cart-empty text-center">
            {%- if settings.cart_type == 'drawer' -%}
            <div class="dropdown-cart_title drawer-cart_title_empty">
              <span>{{ 'sections.dropdown_cart.title' | t}}</span>
              <button type="button" class="btn-close btn-close-cart cursor-pointer" title="{{ 'sections.dropdown_cart.close' | t }}">{%- render 'svg' type: 'close' -%}</button>
            </div>
            {%- endif -%}
            <div class="text">{{ 'sections.dropdown_cart.empty' | t}}</div>
          </div>
        </div>
      </template>
    {%- endif -%}
  {% comment %} comm {% endcomment %}
{%- endcase -%}