      endif
    -%}
    <div class="header-desk d-none d-lg-block">
      <div class="header-top">
        <div class="{{ layout_class }}">
          <div class="header-top_wrapper d-flex justify-content-between">
            {%- if section.settings.textbox_1 != blank or section.settings.textbox_2 != blank -%}
              {%- liquid 
                assign textbox_1 = section.settings.textbox_1
                assign icon_1 = section.settings.icon_1
                assign textbox_2 = section.settings.textbox_2
                assign icon_2 = section.settings.icon_2
              -%}
              <div class="header-contact-box">
                <ul class="list">
                  {%- if textbox_1 != blank -%}
                    <li class="item">
                      {%- if icon_1 != blank -%}{{icon_1}}{%- endif -%}
                      <span>{{ textbox_1 }}</span>
                    </li>
                  {%- endif -%}

                  {%- if textbox_2 != blank -%}
                    <li class="item">
                      {%- if icon_2 != blank -%}{{icon_2}}{%- endif -%}
                      <span>{{ textbox_2 }}</span>
                    </li>
                  {%- endif -%}
                </ul>
              </div>
            {%- endif -%}

            <div class="header-logo-search">
              {%- render 'header-snippet' type: 'logo', section: section -%}
              {%- render 'header-snippet' type: 'search-form' section: section -%}
            </div>

            <div class="header-action-list d-flex">
              {%- if settings.enable_product_compare -%}
                <div class="compare-target">
                  {%- render 'arn_wl_cp_buttons', type: 'cp_show_btn', product_handle: product.handle -%}
