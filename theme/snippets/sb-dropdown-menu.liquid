<li class="dropdown{% if l.active or l.child_active %} s-open active{% endif %}">
  <a href="{{ l.url }}" class="dropdown-link"><span>{{ l.title }}</span></a>
  <span class="expand">{%- render 'svg', type: 'angle-right' -%}</span>

  <ul class="dropdown-menu">
    {% for l in linklists[submenu].links %}
      {% render 'sb-nav-sub-link' %}
    {% endfor %}
  </ul>
</li>